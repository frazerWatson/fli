<div id="post_content">
  <h1 class="title">
    <%= @post.title %>
  </h1>

  <p class="date">
    Submitted <%= time_ago_in_words(@post.created_at) %> Ago
      <div id="admin_links">
        <% if user_signed_in? %>
          | <%= link_to 'Edit', edit_post_path(@post) %>
          | <%= link_to 'Delete', post_path(@post), method: :delete, data: { confirm: 'Are you sure?' } %> 
      </div>
       <% end %>
  </p>


  
  <div id="pan_img">
  <p><%= image_tag @post.image.url(:medium), class:"pan_img" %></br></p>
  </div>
    

  <p class="body">
    <%= @post.body %>
  </p> 

  <p><%= image_tag "logo.svg" %></p>
  <p><%= image_tag "logo.svg" %></p>
  <p><%= image_tag "logo.svg" %></p>

  <%= render 'disqus' %>


</div>